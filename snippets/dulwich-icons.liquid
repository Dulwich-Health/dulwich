{% comment %}
  Renders dulwich icons.
  Accepts:
  - icon_name: {String} Icon Name.
  - class: {String} Icon Class.

  Usage:
  {% render 'dulwich-icons', icon_name: icon_name, class: class %}

   Example:
   {%- render 'dulwich-icons',
    icon_name: 'quality-icon',
    class: 'tw-w-[2rem] tw-h-[2rem] md:tw-w-[2.4rem] md:tw-h-[2.4rem]'
  -%}
{% endcomment %}

{% case icon_name %}
  {% when 'enlarge-image' %}
    <svg xmlns="http://www.w3.org/2000/svg" width="55" height="54" viewBox="0 0 55 54" fill="none">
      <path d="M23.0214 30.4144L13.5306 39.9052" stroke="#60919F" stroke-width="2.56423" stroke-linecap="round" stroke-linejoin="round"/>
      <path d="M21.8372 39.9052L13.5327 39.9052L13.5327 31.6007" stroke="#60919F" stroke-width="2.56423" stroke-linecap="round" stroke-linejoin="round"/>
      <path d="M30.8092 22.6328L40.3 13.1419" stroke="#60919F" stroke-width="2.56423" stroke-linecap="round" stroke-linejoin="round"/>
      <path d="M31.9956 13.14L40.3 13.14L40.3 21.4445" stroke="#60919F" stroke-width="2.56423" stroke-linecap="round" stroke-linejoin="round"/>
      <circle cx="27.4577" cy="27.0682" r="25.4802" stroke="#60919F" stroke-width="2.56423"/>
    </svg>

{% endcase %}