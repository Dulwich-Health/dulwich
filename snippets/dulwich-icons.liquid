{% comment %}
  Renders dulwich icons.
  Accepts:
  - icon_name: {String} Icon Name.
  - class: {String} Icon Class.

  Usage:
  {% render 'dulwich-icons', icon_name: icon_name, class: class %}

   Example:
   {%- render 'dulwich-icons',
    icon_name: 'quality-icon',
    class: 'tw-w-[2rem] tw-h-[2rem] md:tw-w-[2.4rem] md:tw-h-[2.4rem]'
  -%}
{% endcomment %}

{% case icon_name %}
  {% when 'enlarge-image' %}
    <svg xmlns="http://www.w3.org/2000/svg" width="50" height="51" viewBox="0 0 50 51" fill="none">
      <path d="M21.3203 28.5176L12.626 37.2119" stroke="#242424" stroke-width="2.48978" stroke-linecap="round" stroke-linejoin="round"/>
      <path d="M20.2354 37.2119L12.6278 37.2119L12.6278 29.6044" stroke="#242424" stroke-width="2.48978" stroke-linecap="round" stroke-linejoin="round"/>
      <path d="M28.4551 21.3887L37.1494 12.6944" stroke="#242424" stroke-width="2.48978" stroke-linecap="round" stroke-linejoin="round"/>
      <path d="M29.5419 12.6923L37.1494 12.6923L37.1494 20.2998" stroke="#242424" stroke-width="2.48978" stroke-linecap="round" stroke-linejoin="round"/>
      <circle cx="25.3846" cy="25.451" r="23.3417" stroke="#242424" stroke-width="2.48978"/>
    </svg>

{% endcase %}